{
  "cSpell.words": [
    "antfu",
    "commitlint",
    "dcloudio",
    "pinia",
    "snsapi",
    "stylelint",
    "tailwindcss",
    "twucss",
    "unocss",
    "uview"
  ],
  "prettier.enable": false,
  "editor.formatOnSave": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.organizeImports": false
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "vue",
    "html",
    "markdown",
    "json",
    "jsonc",
    "yaml"
  ]
}
